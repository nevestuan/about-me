{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "baseUrl": ".",
        "paths": {
            "@components/*": ["src/modules/componentExamples/*"],
            "@components": ["src/modules/componentExamples"],
            "@components/icons/*": ["src/modules/componentExamples/icons/*"],
            "@components/icons": ["src/modules/componentExamples/icons"],
            "@core/*": ["src/modules/core/*"],
            "@tridi/*": ["src/modules/tridi/*"],
            "@home/*": ["src/modules/home/*"],
            "@public/*": ["public/*"],
            "@public": ["public"],
            "@definitions/*": ["src/definitions/*"],
            "@definitions": ["src/definitions"],
            "@interfaces/*": ["src/interfaces/*"],
            "@interfaces": ["src/interfaces"],
            "@styles/*": ["src/styles/*"],
            "@styles": ["src/styles"],
            "@redux/*": ["src/redux/*"],
            "@redux": ["src/redux"],
            "@test/*": ["test/*"],
            "@test": ["test"]
        }
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        "src/styles/globalStyles.js"
, "src/modules/tridi/demo/TridiDemo.js"    ],
    "exclude": ["node_modules", "test/*", "webdriverio/*"]
}
